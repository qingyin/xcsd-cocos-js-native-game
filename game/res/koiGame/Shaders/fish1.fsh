#ifdef GL_ES	varying mediump vec2 v_texCoord;	varying lowp vec4 v_color;#else	varying vec2 v_texCoord;	varying vec4 v_color;#endif//uniform sampler2D CC_Texture0;void main() {    vec4 sourceColor = texture2D(CC_Texture0, v_texCoord);    vec3 tint = sourceColor.x * vec3(0.99, 0.7, 0.1) + sourceColor.y * vec3(0.98, 0.93, 0.72);    gl_FragColor = v_color * vec4(mix(tint, vec3(1,1,1), sourceColor.z), sourceColor.w);}		